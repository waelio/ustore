{
  "name": "@waelio/universal-store",
  "version": "1.0.0-alpha-1",
  "description": "Universal Storage",
  "main": "lib/cjs/index.js",
  "module": "lib/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "rm -rf lib/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir lib/cjs",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "bin": {
    "uStore": "lib/index.js"
  },
  "directories": {
    "lib": "/lib",
    "types": "types/index.d.ts"
  },
  "files": [
    "lib/",
    "types/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waelio/universal-store.git"
  },
  "keywords": [
    "Store",
    "localStorage",
    "cookies",
    "vuex",
    "redux",
    "pinia",
    "gun"
  ],
  "sideEffects": false,
  "author": {
    "name": "Wael Wahbeh",
    "email": "wahbehw@me.com",
    "url": "https://github.com/waelio/universal-store#readme"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/waelio/universal-store/issues"
  },
  "homepage": "https://github.com/waelio/universal-store#readme",
  "devDependencies": {
    "@babel/parser": "^7.19.0",
    "@types/jest": "^29.0.0",
    "axios": "^0.21.2",
    "core-js": "^3.25.1",
    "gun": "0.2020.1238",
    "jest": "^29.0.3",
    "jest-puppeteer": "^6.1.1",
    "pinia": "2.0.22",
    "prettier": "^2.7.1",
    "regenerator-runtime": "^0.13.9",
    "store2": "^2.14.2",
    "ts-jest": "^29.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.3",
    "vue": "3.2.39",
    "vuex": "4.0.2",
    "waelio-utils": "4.0.2"
  }
}