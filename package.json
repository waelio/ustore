{
  "name": "@waelio/ustore",
  "version": "0.0.25",
  "description": "Universal Storage",
  "main": "lib/index.js",
  "module": "commonjs",
  "types": "lib/types.js",
  "exports": {
    ".": {
      "browser": {
        "default": "./lib/index.js"
      }
    }
  },
  "public": "true",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "rm -rf lib/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir lib/cjs",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "npx eslint",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "bin": {
    "uStore": "lib/index.js"
  },
  "directories": {
    "lib": "lib",
    "src": "src"
  },
  "files": [
    "src/",
    "lib/",
    "types/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waelio/ustore.git"
  },
  "keywords": [
    "Store",
    "localStorage",
    "cookies",
    "vuex",
    "redux",
    "pinia",
    "gun"
  ],
  "sideEffects": false,
  "author": "Wael Wahbeh <wahbehw@me.com> (https://github.com/waelio/universal-store#readme)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/waelio/universal-store/issues"
  },
  "homepage": "https://github.com/waelio/universal-store#readme",
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.8",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "gun": "0.2020.1238",
    "pinia": "2.0.23",
    "store2": "^2.14.2",
    "vue": "3.2.41",
    "vue-demi": "^0.13.11",
    "vuex": "4.1.0",
    "waelio-utils": "5.0.0"
  }
}
