{
  "name": "@waelio/ustore",
  "version": "0.0.93",
  "description": "Universal Storage",
  "homepage": "https://github.com/waelio/uStore#readme",
  "keywords": [
    "Store",
    "localStorage",
    "cookies",
    "vuex",
    "redux",
    "pinia",
    "gun"
  ],
  "main": "./lib/src/index.js",
  "types": "./types/index.d.ts",
  "public": "true",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "require": "./lib/cjs/src/index.js",
      "import": "./lib/src/index.js"
    }
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "resolutions": {
    "vue-demi": "0.13.11"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "rm -rf lib && npm run build:esm && npm run build:cjs && npm run build:umd",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir lib/cjs",
    "build:umd": "tsc --module umd --outDir lib/umd",
    "format": "prettier --write \".//src/**/*.ts\"",
    "lint": "npx eslint",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "src/**/*",
    "lib/**/*",
    "types/**/*"
  ],
  "bin": {
    "uStore": "./lib/umd/src/index.js"
  },
  "author": {
    "name": "Wael Wahbeh",
    "email": "wahbehw@me.com",
    "url": "https://waelio.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waelio/ustore.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/waelio/uStore/issues"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.0",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "jest": "^29.3.0",
    "jest-environment-jsdom": "^29.3.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "babel-plugin-transform-require-context": "^0.1.1",
    "browserify": "^17.0.0",
    "gun": "0.2020.1238",
    "localforage": "^1.10.0",
    "pinia": "2.0.23",
    "store2": "^2.14.2",
    "vue": "3.2.47",
    "vue-demi": "0.13.11",
    "vuex": "4.1.0",
    "waelio-utils": "5.0.1"
  }
}
